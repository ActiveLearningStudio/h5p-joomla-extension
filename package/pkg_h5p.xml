	<files folder="packages">
		<file type="component" id="com_h5p">com_h5p.zip</file>
		<file type="library" id="h5pcore">lib_h5pcore.zip</file>
		<file type="plugin" id="h5p" group="content">plg_content_h5p.zip</file>
		<file type="plugin" id="h5p" group="user">plg_user_h5p.zip</file>
		<file type="plugin" id="h5p" group="editors-xtd">plg_editors_xtd_h5p.zip</file>
